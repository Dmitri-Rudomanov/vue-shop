    "vue-universal-modal": "^1.1.4",
    "vuedraggable": "^2.24.3",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
